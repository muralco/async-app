{
  "name": "async-app",
  "version": "4.10.0",
  "description": "An express wrapper for handling async middlewares, order middlewares, schema validator, and other stuff",
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./analyze": "./dist/analyze.js",
    "./error": "./dist/error.js",
    "./swagger": "./dist/swagger.js",
    "./types": "./dist/types.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/muralco/async-app.git"
  },
  "author": "MURAL",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/muralco/async-app/issues"
  },
  "homepage": "https://github.com/muralco/async-app",
  "keywords": [
    "express",
    "async",
    "schema"
  ],
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/cucumber": "4.0.4",
    "@types/express": "^4.16.0",
    "@types/lodash": "^4.14.116",
    "@types/node": "10.5.2",
    "@types/supertest": "^2.0.6",
    "body-parser": "^1.18.3",
    "mural-schema": "^1.0.2",
    "pickled-cucumber": "^1.0.6",
    "supertest": "^3.3.0",
    "ts-node": "^7.0.1",
    "ts-unused-exports": "^2.0.11",
    "tslint": "5.11.0",
    "tslint-config-airbnb": "5.9.2",
    "typescript": "3.0.3"
  },
  "dependencies": {
    "express": "^4.16.3",
    "lodash": "^4.17.11"
  },
  "scripts": {
    "build": "tsc",
    "dist": "rm -rf dist && npm run build",
    "lint": "tslint -p .",
    "lint:fix": "tslint -p . --fix",
    "test": "cucumber-js --require-module ts-node/register --exit -r src/test.ts",
    "test:debug": "node --inspect-brk=0.0.0.0:9429 --no-lazy ./node_modules/.bin/cucumber-js --require-module ts-node/register --exit -r src/test.ts",
    "watch": "tsc -w",
    "prepack": "npm run test",
    "prepare": "npm run dist"
  }
}
